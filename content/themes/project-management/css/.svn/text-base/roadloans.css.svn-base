body {
	font: 1em/1.625 "Trebuchet MS", Tahoma, Arial, Sans-serif;
}
a { 
	color: #224D73;
	text-decoration: underline;
}

.button.success {
background: #ffc600; /* Old browsers */
background: -moz-linear-gradient(top,  #ffc600 0%, #fea100 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#ffc600), color-stop(100%,#fea100)); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top,  #ffc600 0%,#fea100 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top,  #ffc600 0%,#fea100 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top,  #ffc600 0%,#fea100 100%); /* IE10+ */
background: linear-gradient(to bottom,  #ffc600 0%,#fea100 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffc600', endColorstr='#fea100',GradientType=0 ); /* IE6-9 */
color: #193E58;
text-shadow: 1px 1px 1px #FFD673;
}

.button.success:hover {
background: #fea100; /* Old browsers */
background: -moz-linear-gradient(top,  #fea100 0%, #ffc600 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#fea100), color-stop(100%,#ffc600)); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top,  #fea100 0%,#ffc600 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top,  #fea100 0%,#ffc600 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top,  #fea100 0%,#ffc600 100%); /* IE10+ */
background: linear-gradient(to bottom,  #fea100 0%,#ffc600 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#fea100', endColorstr='#ffc600',GradientType=0 ); /* IE6-9 */
}

.button, .button.primary {
background: #154a70; /* Old browsers */
background: -moz-linear-gradient(top,  #154a70 0%, #193e5a 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#154a70), color-stop(100%,#193e5a)); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top,  #154a70 0%,#193e5a 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top,  #154a70 0%,#193e5a 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top,  #154a70 0%,#193e5a 100%); /* IE10+ */
background: linear-gradient(to bottom,  #154a70 0%,#193e5a 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#154a70', endColorstr='#193e5a',GradientType=0 ); /* IE6-9 */
color:#fff;
}

.button:hover, .button.primary {
background: #193e5a; /* Old browsers */
background: -moz-linear-gradient(top,  #193e5a 0%, #154a70 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#193e5a), color-stop(100%,#154a70)); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top,  #193e5a 0%,#154a70 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top,  #193e5a 0%,#154a70 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top,  #193e5a 0%,#154a70 100%); /* IE10+ */
background: linear-gradient(to bottom,  #193e5a 0%,#154a70 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#193e5a', endColorstr='#154a70',GradientType=0 ); /* IE6-9 */
}

.button.secondary {
background: #e8e8e8; /* Old browsers */
background: -moz-linear-gradient(top,  #e8e8e8 0%, #cccccc 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#e8e8e8), color-stop(100%,#cccccc)); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top,  #e8e8e8 0%,#cccccc 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top,  #e8e8e8 0%,#cccccc 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top,  #e8e8e8 0%,#cccccc 100%); /* IE10+ */
background: linear-gradient(to bottom,  #e8e8e8 0%,#cccccc 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#e8e8e8', endColorstr='#cccccc',GradientType=0 ); /* IE6-9 */
color:#00679c;
border:2px solid #00679c;
}
.button.secondary:hover {
background: #cccccc; /* Old browsers */
background: -moz-linear-gradient(top,  #cccccc 0%, #e8e8e8 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#cccccc), color-stop(100%,#e8e8e8)); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top,  #cccccc 0%,#e8e8e8 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top,  #cccccc 0%,#e8e8e8 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top,  #cccccc 0%,#e8e8e8 100%); /* IE10+ */
background: linear-gradient(to bottom,  #cccccc 0%,#e8e8e8 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#cccccc', endColorstr='#e8e8e8',GradientType=0 ); /* IE6-9 */
}